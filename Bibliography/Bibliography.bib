@Book{cascales_libro_2003,
	title = {El libro de {Latex}},
	isbn = {978-84-205-3779-5},
	abstract = {Esta obra es un amplio manual del sistema de composición de textos denominado LATEX, consistente en un conjunto de herramientas construidas sobre la base del compilador TEX. El lector que no conozco en absoluto LATEX se enfrentará a una quincena de amigables lecciones, todas ellas con un espíritu eminentemente práctico y cada una centrada en un elemento específico de la composición de textos. Tras la práctica de estas lecciones, que constituyen la primera parte de la obra, el lector estará en disposición de escribir todo tipo de documentos, tanto para ser impresos como para ser visualizados en un ordenador (por ejemplo, publicados en Internet), con una calidad muy superior a la proporcionada por otros sistemas de edición. Para aquellos que ya posean esa experiencia, la segunda parte de la obra proporciona conocimientos en amplitud y profundidad suficientes para personalizar los textos, así como la solución, o al menos una pista para la solución, de casi todos los posibles problemas que puedan aparecer en este arte de la composición de textos.},
	language = {es},
	publisher = {Pearson Educación},
	author = {Cascales, Bernardo and Salinas, Bernardo Cascales},
	year = {2003},
	note = {Google-Books-ID: haLgOwAACAAJ},
	keywords = {latex},
}

@Book{cascales_salinas_latex_2000,
	title = {{LaTeX}: una imprenta en sus manos},
	isbn = {978-84-920381-9-0},
	shorttitle = {{LaTeX}},
	abstract = {LaTeX es un lenguaje de macros creado en 1982 por Leslie Lamport para facilitar la composición de textos con el compilador TeX que había aparecido en 1978 de la mano de Donald Ervin Knuth en la Universidad de Stanford. Pero ¿es, LaTeX la herramienta adecuada para mí?, se estará preguntando. Lo es, sin duda, si necesita escribir documentos científicos o técnicos, con fórmulas matemáticas, aunque sean pocas. También lo es si necesita escribir un libro, técnico o no, o en cualquier otro caso, si gusta de escribir documentos de la más alta calidad posible. LaTeX es también, sin duda, su herramienta si sus documentos electrónicos tienen que viajar por Internet como anexos de sus mensajes de correo electrónico pues los ficheros producidos por LaTeX son de tamaño muy reducido. Si usted prefiere que sus documentos no viajen por la red pero que sean transformados a formato HTML o PDF para ser leídos directamente en su servidor web, LaTeX también le resolverá el problema. Actualmente LaTeX es un producto muy evolucionado con respecto a la versión original de Leslie Lamport y a su espectacular auge ha contribuido el que es un producto gratuito, de gran calidad, de gran flexibilidad, que nació para adaptarse constantemente a las evoluciones informáticas, que es utilizado en plan profesional por muchas empresas editoriales y que muchas personas producen constantemente paquetes que resuelven las necesidades de casi todos sus potenciales usuarios. Cuando usted escriba con LaTeX, éste se encargará por si sólo de dar formato al documento; elegir el tipo de letra a utilizar según el contexto; separar cabeceras y texto; realizar sangrados donde proceda; justificar el texto, insertando guiones automáticamente y evitando que en una línea los espacios en blanco sean demasiado grandes o demasiado pequeños; decidir donde colocar figuras y tablas (como objetos flotantes) para que la impresión visual de un texto que las contenga sea "profesional"; llevar todos los contadores de páginas, capítulos, secciones, etc. y realizar de forma automática las referencias cruzadas; generar automáticamente unos índices generales e índices terminológicos; generar listas bibliográficas a partir de bases de datos de bibliografía; importar todo tipo de gráficos; etc. Esta obra representa el más ambicioso proyecto editorial acerca de LaTeX en español. Nosotros, sus autores, gozamos de una larga experiencia en el manejo de LaTeX y alcanzamos el objetivo de escribirla a raíz de los cursos de postgrado que impartimos en la Universidad de Murcia. Para el primero de estos cursos escribimos un pequeño manual, germen del resultado que tiene en sus manos, que se ha visto así enriquecido con la experiencia de sucesivos cursos y aportaciones de nuestros alumnos. Con el mismo espíritu de LaTeX de depuración y evolución constante queremos que este libro sea un producto vivo que ahora se enriquezca de los comentarios y sugerencias de nuestros lectores, a disposición de los que ponemos la dirección de correo electrónico latex@um.es y el servidor http://www.latex.um.es. LaTeX una imprenta en sus manos ha sido mecanografiado, compuesto y maquetado por sus autores con LaTeX. Lo que usted ve en este libro es lo impreso por la filmadora sobre el producto final entregado por los autores.},
	language = {es},
	publisher = {Aula Documental de Investigación},
	author = {Cascales Salinas, Bernardo and {Lucas Saorin, Pascual} and {Mira Rios, José Manuel} and Pallarés Ruíz, Antonio and {Sánchez-Pedreño Guillén, Salvador}},
	year = {2000},
	note = {Google-Books-ID: hv29AAAACAAJ},
	keywords = {latex},
}

